<div id="pixelmap-legend">
    <span>Normalized value: 0</span>
    <div class="item" data="0.1" title="0.1"></div>
    <div class="item" data="0.2" title="0.2"></div>
    <div class="item" data="0.3" title="0.3"></div>
    <div class="item" data="0.4" title="0.4"></div>

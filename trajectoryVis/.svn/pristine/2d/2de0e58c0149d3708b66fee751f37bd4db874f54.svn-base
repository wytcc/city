body,div,dl,dt,dd,ul,ol,li,h1,h2,h3,h4,h5,h6,pre,code,form,fieldset,legend,input,textarea,p,blockquote,th,td{margin:0;padding:0}table{border-collapse:collapse;border-spacing:0}fieldset,img{border:0}html,body{height:100%}body,button,input,select,textarea{font:14px Helvetica, Arial, Microsoft YaHei}address,caption,cite,code,dfn,em,strong,th,var{font-style:normal;font-weight:normal}ol,ul{list-style:none}caption,th{text-align:left}h1,h2,h3,h4,h5,h6{font-size:100%;font-weight:normal}q:before,q:after{content:''}abbr,acronym{border:0;font-variant:normal}sup{vertical-align:text-top}sub{vertical-align:text-bottom}input,textarea,select{font-family:inherit;font-size:inherit;font-weight:inherit;*font-size:100%}legend{color:#000}h1{font-size:31px;color:#4A90E2}h2{font-size:20px;color:#4A90E2}input:-ms-input-placeholder,textarea:-ms-input-placeholder{color:#D1D1D1}a:focus{outline:none}p,span{cursor:default}::-webkit-scrollbar{width:6px;height:6px}::-webkit-scrollbar-thumb{background:rgba(97,97,97,0.7)}body{margin:0}html{width:100%;height:100%}body{width:100%;height:100%}.container{width:100%;height:100%}.container #map{width:100%;height:60%}.container #timeline{width:100%;height:40%}.container #timeline svg{width:100%;height:100%}.ui-brush{background:#f8f8f8;position:absolute;bottom:0;right:0;left:0;height:100px}.brush .extent{stroke:#fff;fill-opacity:0.125;shape-rendering:crispEdges}
